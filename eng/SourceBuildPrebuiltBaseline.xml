<UsageData>
  <IgnorePatterns>
    <!-- Re-enable with https://github.com/dotnet/source-build/issues/2545 -->
    <UsagePattern IdentityGlob="*/*" />

    <!-- Sourcelink prebuilts. See https://github.com/dotnet/source-build/issues/2515 -->
    <UsagePattern IdentityGlob="Microsoft.SourceLink.GitHub/*" />
    <UsagePattern IdentityGlob="Microsoft.SourceLink.Common/*" />
    <UsagePattern IdentityGlob="Microsoft.Build.Tasks.Git/*" />
    <UsagePattern IdentityGlob="Microsoft.SourceLink.AzureRepos.Git/*" />

    <!-- runtime prebuilts.  Update when runtime produces intermediate packages correctly. -->
    <UsagePattern IdentityGlob="runtime.linux-x64.Microsoft.NETCore.ILAsm/*" />
    <UsagePattern IdentityGlob="runtime.linux-x64.Microsoft.NETCore.ILDAsm/*" />

    <UsagePattern IdentityGlob="Microsoft.SourceBuild.Intermediate.*/*" />

    <UsagePattern IdentityGlob="runtime.*/*" />
  </IgnorePatterns>
  <Usages/>
</UsageData>